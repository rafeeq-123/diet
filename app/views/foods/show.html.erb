<%Food.all.each do |food| %>
<%=food.dish%>
<%= food.food_groups%>
<%= food.created_at.strftime("%B,%d")%>

<p><%=render "amounts/form"%></p>

<%= link_to "Edit", edit_food_path%>
<%= link_to "Return to Home", root_path%>
<%= link_to "Delete", food_path(@food), :method => :delete, :confirm =>"Are you sure Africa?"%>


<%end%>